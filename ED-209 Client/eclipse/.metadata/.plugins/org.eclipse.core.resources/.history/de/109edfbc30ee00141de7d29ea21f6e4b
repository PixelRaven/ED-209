package net.pixelraven.ed209.module.modules;

import net.minecraft.client.Minecraft;
import net.minecraft.network.play.client.C03PacketPlayer.C06PacketPlayerPosLook;
import net.minecraft.network.play.server.S38PacketPlayerListItem.Action;
import net.pixelraven.ed209.module.Category;
import net.pixelraven.ed209.module.Module;

public class Derp extends Module{
	public Derp() {
		super("Derp", Category.PLAYER);
		setUsage("Derp();");
		setBlockedState(3);
	}
	
	public void onUpdate() {
		if(!this.getToggled())
			return;
		Minecraft.getMinecraft().thePlayer.sendQueue.addToSendQueue(new C06PacketPlayerPosLook(Math.random()*360, Math.random()*360, Math.random()*360, (float)Math.random()*360, (float)Math.random()*360, true));
	}
}
