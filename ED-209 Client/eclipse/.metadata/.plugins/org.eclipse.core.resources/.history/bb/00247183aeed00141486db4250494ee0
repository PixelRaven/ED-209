package disconnected.module.modules;

import net.minecraft.client.Minecraft;
import disconnected.module.Category;
import disconnected.module.Module;

public class Disconnect extends Module{
	public Disconnect() {
		super("Disconnect", Category.OTHER);
		setUseType(1);
		setUsage("Disconnect();");
	}
	
	public void onUse() {
		Minecraft.getMinecraft().stopIntegratedServer();
	}
}
