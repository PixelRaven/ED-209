package disconnected.module.modules;

import net.minecraft.client.Minecraft;
import disconnected.module.Category;
import disconnected.module.Module;

public class NoBlind extends Module{
	public NoBlind() {
		super("No Blind", Category.PLAYER);
		setUsage("NoBlind();");
		setBlockedState(3);
	}
	
	public void onUpdate() {
		if(!this.getToggled())
			return;
		
		 Minecraft.getMinecraft().thePlayer.removePotionEffectClient(15);
	}
}
